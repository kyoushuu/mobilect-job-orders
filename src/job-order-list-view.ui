<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
  <object class="GtkOverlay" id="overlay">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <child>
      <object class="GtkScrolledWindow" id="scrolledwindow">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <child>
          <object class="GtkTreeView" id="treeview">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="headers_visible">False</property>
            <property name="reorderable">True</property>
            <property name="enable_search">False</property>
            <property name="activate_on_single_click">True</property>
            <style>
              <class name="content-view"/>
            </style>
            <signal name="row-activated" handler="mpcjo_job_order_list_view_on_treeview_row_activated" swapped="no"/>
            <child internal-child="selection">
              <object class="GtkTreeSelection" id="treeview-selection_results">
                <property name="mode">none</property>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="treeviewcolumn_selected">
                <property name="min_width">50</property>
                <property name="title" translatable="yes">Selected</property>
                <property name="alignment">0.5</property>
                <child>
                  <object class="GtkCellRendererToggle" id="cellrenderertoggle_selected"/>
                  <attributes>
                    <attribute name="active">13</attribute>
                  </attributes>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="treeviewcolumn_job_order">
                <property name="min_width">50</property>
                <property name="title" translatable="yes">Job Order</property>
                <property name="alignment">1</property>
                <child>
                  <object class="GtkCellRendererText" id="cellrenderertext_job_order_number">
                    <property name="xpad">10</property>
                    <property name="foreground_gdk">#88888a8a8585</property>
                    <property name="width_chars">7</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="treeviewcolumn_customer">
                <property name="min_width">250</property>
                <property name="title" translatable="yes">Customer</property>
                <property name="expand">True</property>
                <child>
                  <object class="GtkCellRendererText" id="cellrenderertext_customer">
                    <property name="ellipsize">end</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="treeviewcolumn_date">
                <property name="min_width">150</property>
                <property name="title" translatable="yes">Date</property>
                <property name="alignment">1</property>
                <child>
                  <object class="GtkCellRendererText" id="cellrenderertext_date">
                    <property name="alignment">right</property>
                    <property name="foreground_gdk">#88888a8a8585</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkTreeViewColumn" id="treeviewcolumn_purchase_order">
                <property name="min_width">125</property>
                <property name="title" translatable="yes">Purchase Order</property>
                <child>
                  <object class="GtkCellRendererText" id="cellrenderertext_purchase_order_number"/>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
