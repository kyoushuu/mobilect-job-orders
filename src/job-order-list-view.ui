<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.8 -->
  <object class="GtkButton" id="button_print">
    <property name="visible">True</property>
    <property name="halign">end</property>
    <property name="valign">center</property>
    <property name="label">Print</property>
    <signal name="clicked" handler="mpcjo_job_order_list_view_on_button_print_clicked" swapped="no"/>
    <style>
      <class name="text-button"/>
    </style>
  </object>
  <object class="GtkTreeViewColumn" id="treeviewcolumn_job_order">
    <property name="expand">True</property>
    <property name="title" translatable="yes">Job Order</property>
    <child>
      <object class="GtkCellRendererText" id="cellrenderertext_job_order_number"/>
    </child>
  </object>
  <object class="GtkTreeViewColumn" id="treeviewcolumn_purchase_order">
    <property name="expand">True</property>
    <property name="title" translatable="yes">Purchase Order</property>
    <child>
      <object class="GtkCellRendererText" id="cellrenderertext_purchase_order_number"/>
    </child>
  </object>
  <object class="GtkTreeViewColumn" id="treeviewcolumn_invoice">
    <property name="expand">True</property>
    <property name="title" translatable="yes">Invoice</property>
    <child>
      <object class="GtkCellRendererText" id="cellrenderertext_invoice_number"/>
    </child>
  </object>
  <object class="GtkTreeViewColumn" id="treeviewcolumn_payment">
    <property name="expand">True</property>
    <property name="title" translatable="yes">Payment</property>
    <child>
      <object class="GtkCellRendererText" id="cellrenderertext_payment"/>
    </child>
  </object>
</interface>
